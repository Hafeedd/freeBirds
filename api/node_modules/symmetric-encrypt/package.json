    "argon2",
    "pbkdf2",
    "bcrypt",
    "scrypt",
    "cipher"
  ],
  "author": "Kincaid O'Neil (https://kincaidoneil.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kincaidoneil/encrypt-it/issues"
  },
  "homepage": "https://github.com/kincaidoneil/encrypt-it#readme",
  "dependencies": {
    "libsodium-wrappers": "^0.7.4"
  },
  "devDependencies": {
    "@types/libsodium-wrappers": "^0.7.5",
    "@types/node": "^11.13.4",
    "prettier": "^1.17.0",
    "standard": "^12.0.1",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.3"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
